<div class="container">
<div class="col-xs-12">
<%= simple_form_for @member, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  
  <%= f.input :last_fee_date %>
  
  <%= f.input :next_fee_date %>
  
  <%= f.input :status %>
  
  <%= f.input :amount %>
  

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                members_path, :class => 'btn btn-default' %>
<% end %>
</div>
</div>